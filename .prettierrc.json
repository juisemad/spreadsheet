{
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": false,
  "bracketSameLine": false,
  "endOfLine": "auto",
  "arrowParens": "always",
  "importOrder": [
    "^react(.*)",
    "<THIRD_PARTY_MODULES>",
    "^@mui/(.*)$",
    "^\\.((?!.styles).)*$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
